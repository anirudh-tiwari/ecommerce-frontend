iv>\n    // )\n    console.log(\"ani\", props.userData)\n    return (\n        //  props.userData?.loading ? (<h2>Loading</h2>) : (\n        <div>\n            <h1>User List</h1>\n            {/* {props.userData?.map(user => <h2>{user.name}</h2>)} */}\n            {props.userData && props.userData?.map(user => <h2>{user.name}</h2>)}\n        </div>\n    )\n    // )\n}\n\n\nconst mapStateToProps = state => {\n    console.log(\"asdasd asdasdasd asdasdasd\", state)\n    return {\n        userData: state?.users\n    }\n}\n\nconst mapDispatchToProps = dispatch => {\n\n    return {\n        fetchUsers: () => dispatch(fetchProduct())\n    }\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(User)\n\n\n","/mnt/c/Users/91991/Documents/ecommerce-frontend/src/redux/product/productReducer.js",["126"],"import {\n    FETCH_Product_REQUEST,\n    FETCH_Product_SUCCESS,\n    FETCH_Product_FAILURE,\n    REMOVE_FROM_CART,\n    EMPTY_CART,\n    ADD_QUANTITY,\n    SUB_QUANTITY\n} from \"./productTypes\"\n\nconst initialState = {\n    loading: false,\n    product: [],\n    error: ''\n};\n\nconst productReducer = (state = initialState, action) => {\n    const { payload, type } = action\n    switch (type) {\n        case FETCH_Product_REQUEST:\n            return {\n                ...state,\n                loading: true\n            }\n        case FETCH_Product_SUCCESS:\n            var cartItem = {\n                quantity: 1,\n                price: payload.DISCOUNT_PRICE,\n                productId: payload.ID,\n                product: payload\n            }\n            return {\n                loading: false,\n                product: [...state.product, cartItem],\n                error: ''\n            }\n        case ADD_QUANTITY:\n            return {\n                ...state,\n                product: state.product.map(products =>\n                    products.product.ID === action.id\n                        ? { ...products, quantity: products.quantity + 1, price: (products.quantity + 1) * products.product.DISCOUNT_PRICE }\n                        : products,\n                ),\n            };\n        case REMOVE_FROM_CART:\n            return {\n                ...state,\n                product: state.product.filter(products =>\n                    products.id == action.id\n                ),\n            };\n        case SUB_QUANTITY:\n            return {\n                ...state,\n                product: state.product.map(products =>\n                    products.product.ID === action.id\n                        ? {\n                            ...products,\n                            quantity: products.quantity !== 1 ? products.quantity - 1 : 1,\n                            price: products.price !== 0 ? products.product.DISCOUNT_PRICE : (products.quantity - 1) * products.price\n                            // price: (products.quantity - 1) * products.price\n                        }\n                        : products,\n                ),\n            };\n        case EMPTY_CART:\n            return {\n                ...state,\n                product: state.product.filter(products =>\n                    products.id === 0\n                )\n            };\n        case FETCH_Product_FAILURE:\n            return {\n                loading: false,\n                product: [],\n                error: payload\n            }\n        default: return state\n\n    }\n}\n\nexport default productReducer;\n","/mnt/c/Users/91991/Documents/ecommerce-frontend/src/redux/product/productTypes.js",[],"/mnt/c/Users/91991/Documents/ecommerce-frontend/src/redux/product/productActions.js",[],"/mnt/c/Users/91991/Documents/ecommerce-frontend/src/redux/rootReducer.js",[],"/mnt/c/Users/91991/Documents/ecommerce-frontend/src/cmp/footer.js",["127"],"/mnt/c/Users/91991/Documents/ecommerce-frontend/src/cmp/About.js",["128"],{"ruleId":"129","replacedBy":"130"},{"ruleId":"131","replacedBy":"132"},{"ruleId":"133","severity":1,"message":
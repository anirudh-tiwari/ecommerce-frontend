 }\n                Api.postQuantity(data).then(() => {\n                    dispatch(addQuantity(id))    \n                })    \n            }\n        })\n        if(cartBlank){\n            const data = {\n                QUANTITY: 1,\n                PRODUCT_ID: id\n            };\n            Api.postCart(data).then((response) => {\n                dispatch(fetchProduct(id)) \n            }); \n        }\n    }\n    \n    \n    return (\n        <div className=\"cards\">\n            <div className=\"card_image\" style={{ backgroundImage: `url('${props.product.IMAGE}')` }}>\n            </div>\n            <div className=\"card_detail \">\n                <h6 className=\"card_detail_size \">\n                    <b>{props.product.NAME}</b>\n                </h6>\n                <h5 className=\"card_detail_size color \">\n                    <b>{props.product.DESCRIPTION}</b>\n                </h5>\n                <h6 className=\" card_detail_size\">\n                    Original Price{\"  \"}\n                    <b className=\"decoration \">\n                        {props.product.ORIGINAL_PRICE}\n                    </b>\n                </h6>\n                <h6 className=\"card_detail_size \">\n                    Discount Price {props.product.DISCOUNT_PRICE}\n                </h6>\n            </div>\n            <div className=\"cardButton\">\n                <Link\n                    className=\"grey_button\"\n                    to={{\n                        pathname: '/cardview',\n                        state: { productID: props.product.ID }\n                    }}>\n                    Viewss\n                    </Link>\n                <Link\n                    className=\"greenButton\"\n                    onClick={() => cart(props.product.ID)}\n                >\n                    Buy\n                    </Link>\n            </div>\n            {popupState?<Popup color=\"success\" message={messageGreenState} />:null}\n        </div>\n    )\n}\n                   \nexport default Card\n","/mnt/c/Users/91991/Documents/ecommerce-frontend/src/cmp/Navbar.js",[],"/mnt/c/Users/91991/Documents/ecommerce-frontend/src/cmp/cartIcon.js",["164","165"],"import React, { useState } from 'react';\nimport \"../App.css\";\nimport \"bootstrap/dist/css/bootstrap.css\";\nimport \"font-awesome/css/font-awesome.min.css\";\nimport { useHistory } from \"react-router-dom\";\nimport CartHover from \"./cartHover\"\nimport { Link } from 'react-router-dom';\nimport { useSelector } from 'react-redux';\n\nfunction CartIcon() {\n    const userData = useSelector(state => state.product.product)\n    return (\n        <div className=\"cartIconLogo\">\n            <div >\n                {userData.length !== 0 ?\n                    <>\n                        <h1 className=\"cartnumber\">{userData.reduce(function (sum, current) {\n                            return sum + current.QUANTITY;\n                        }, 0)}</h1>\n                        <Link to=\"/cart\" className=\" navbar_cart fa fa-shopping-cart navbarCart\">\n                            <span className=\"navbar_cart_text\"></span>\n                        </Link>\n                    </> :\n                    <Link to=\"/cart\" className=\" navbar_cart fa fa-shopping-cart navbarCart\">\n                        <span className=\"navbar_cart_text\"></span>\n                    </Link>\n                }\n            </div>\n            <div className=\"cartIconHover\">\n                {/* aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa */}\n                <CartHover />\n            </div>\n        </div>\n    )\n}\n\nexport default CartIcon\n","/mnt/c/Users/91991/Documents/ecommerce-frontend/src/cmp/CardView.js",["166"],"/mnt/c/Users/91991/Documents/ecommerce-frontend/src/redux/snackbar/snackbarActions.js",[],"/mnt/c/Users/91991/Documents/ecommerce-frontend/src/redux/snackbar/snackbarTypes.js",[],["167","168"],"/mnt/c/Users/91991/Documents/ecommerce-frontend/src/redux/snackbar/snackbarReducer.js",[],"/mnt/c/Users/91991/Documents/ecommerce-frontend/src/cmp/otp.js",["169"],"import React, { useState } from \"react\";\nimport { useHistory } from \"react-router-dom\";\nimport '../App.css';\nimport 'bootstrap/dist/css/bootstrap.min.css';\nimport Verifyotp from \"./Verifyotp\";\nimport Api from \"../Api\";\n\n\nexport default function OTP() {\n    const [mobile_number, setMobile_number] = useState(\"\");\n    const history = useHistory();\n    const handleSubmit = (event) => {\n        event.preventDefault();\n        const data = {\n            mobile_number,\n        };\n        Api.postOtp(data).then((response) => {\n                history.push({ pathname: \"/verify\", state: { mobile_data: response.data.mobile_number, hash: response.data.hash , value:true } });\n          });\n    }\n\n    return (\n        <div className=\"outer\" >\n            <div className=\"inner\" >         \n                <form onSubmit={handleSubmit}>\n                    <h2>Register</h2>\n\n                    <div class=\"form-group  \">\n                        <label for=\"name\">Mobile Number :</label>\n                        <input type=\"text\" id=\"name\" onChange={(e) => setMobile_number(e.target.value)}\n                            class=\"form-control registerInputField\" placeholder=\"Mobile Number \" />\n                    </div>\n                    <button type=\"Submit\" className=\"btn btn-dark btn-lg btn-block registerButton\">Get OTP</button>\n                </form>\n            </div >\n        </div >\n    )\n}\n","/mnt/c/Users/91991/Documents/ecommerce-frontend/src/cmp/Verifyotp.js",["170","171"],"import React, { useState, useEffect } from \"react\";\nimport { useHistory, useLocation } from \"react-router-dom\";\nimport '../App.css';\nimport 'bootstrap/dist/css/bootstrap.min.css';\nimport Popup from \"./Popup\";\nimport Api from \"../Api\";\n\n\nconst Verifyotp = () => {\n    const [popupGreenState, setPopupGreenState] = useState(false);\n    const [messageGreenState, setMessageGreenState] = useState(\"\");\n    const [popupRedState, setPopupRedState] = useState(false);\n    const [messageRedState, setMessageRedState] = useState(\"\");\n    const [popupBlueState, setPopupBlueState] = useState(false);\n    const [otp, setOtp] = useState(\"\");\n    const [mobile_number, setMobile_number] = useState(\"\");\n    const [hash, setHash] = useState(\"\");\n    const history = useHistory();\n    const location = useLocation();\n    const handleSubmit = (event) => {\n        event.preventDefault();\n        const data = {\n            mobile_number,\n            hash,\n            otp,\n        };\n        Api.verifyOtp(data).then((response) => {\n            if(response.data.verification){\n                setPopupGreenState(true);\n                setMessageGreenState(\"Verfied OTP\");\n                localStorage.setItem(\"accessToken\",JSON.stringify(response.data.accessToken));\n                history.push({ pathname: \"/product\", state: {  value:true } });\n            }else if (response.data.verification==false) {\n                setPopupRedState(true);\n                setMessageRedState(\"Incorrect OTP , Try Again\")\n            }\n            else{\n                setPopupRedState(true);\n                setMessageRedState(\"Timeout Pls Try Again\")\n            }\n      });\n    }\n\n    useEffect(() => {\n        setMobile_number(location.state.mobile_data)\n        setHash(location.state.hash)\n        setPopupBlueState(location.state.value)\n    }, []);\n\n    return (\n        <div className=\"outer\" >\n            <div className=\"inner\" >\n                <form onSubmit={handleSubmit}>\n                    <h3>Enter OTP</h3>\n\n                    <div class=\"form-group  \">\n                        <label for=\"name\">OTP :</label>\n                        <input type=\"text\" id=\"name\" onChange={(e) => setOtp(e.target.value)}\n                            class=\"form-control \" placeholder=\"Enter OTP \" />\n                    </div>\n                    <button type=\"Submit\" className=\"btn btn-dark btn-lg btn-block\">Send OTP</button>\n                </form>\n            </div >\n            {popupBlueState?<Popup color=\"info\" message={`OTP sent on mobile ${mobile_number}`} />:null}\n            {popupGreenState?<Popup color=\"success\" message={messageGreenState} />:null}\n            {popupRedState?<Popup color=\"error\" message={messageRedState} />:null}\n\n        </div >\n    )\n}\n\nexport default Verifyotp","/mnt/c/Users/91991/Documents/ecommerce-frontend/src/cmp/Popup.js",["172"],"import React , {  useEffect } from 'react'\nimport { useSelector, useDispatch } from 'react-redux';\nimport { snackbarUIOpen } from '../redux';\nimport { snackbarUIClose } from '../redux';\nimport Snackbar from '@material-ui/core/Snackbar';\nimport MuiAlert from '@material-ui/lab/Alert';\n\nfunction Alert(props) {\n    return <MuiAlert elevation={6} variant=\"filled\" {...props} />;\n  }\n\nconst Popup = (props) => {\n    console.log(\"nnnnnnnnnnnnnn\",props)\n    const dispatch = useDispatch()\n    const snackbarDataOpen = useSelector(state => state.snackbar.open)\n\n    const handleClose = (event, reason) => {\n        if (reason === 'clickaway') {\n          return;\n        }\n        dispatch(snackbarUIClose())        \n      };\n\n    useEffect(() => {\n        dispatch(snackbarUIOpen())\n    }, [])\n\n    return (\n        <div>\n        <Snackbar open={snackbarDataOpen} autoHideDuration={2000} onClose={handleClose}>\n          <Alert onClose={handleClose} severity={props.color}>\n             {props.message}                      \n          </Alert>\n      </Snackbar>   \n        </div>\n    )\n}\n\nexport default Popup\n\n\n","/mnt/c/Users/91991/Documents/ecommerce-frontend/src/route/protected.js",["173"],{"ruleId":"174","replacedBy":"175"},{"ruleId":"176","replacedBy":"177"},{"ruleId":"178","severity":1,"message":"179","line":